{
    "SENDMORGUE_BASEADDRESS": "marsundiscovered.azurewebsites.net",
    "SENDMORGUE_ENDPOINT": "api/morgue",
    "Serilog": {
        // Log levels include:
        //     Verbose, Debug, Information, Warning, Error, Fatal
        "MinimumLevel": {
            "Default": "Warning"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "MarsUndiscovered.log"
                }
            }
        ],
        "Enrich": [ "FromLogContext", "WithExceptionDetails" ]
    }
}